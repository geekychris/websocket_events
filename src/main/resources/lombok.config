# This file is generated by the 'lombok' configuration.

# Tells Lombok that this is the root directory and that it shouldn't search parent directories for more configuration files
config.stopBubbling = true

# Add @Generated annotation to all generated methods
lombok.addLombokGeneratedAnnotation = true

# Add @java.beans.ConstructorProperties to all generated constructors
lombok.anyConstructor.addConstructorProperties = true

# Don't use getter names in toString methods
lombok.toString.doNotUseGetters = true

# Generate proper equals/hashCode methods
lombok.equalsAndHashCode.callSuper = call

# Additional settings for better IDE support
lombok.log.fieldName = log
lombok.accessors.chain = true
lombok.addLombokGeneratedAnnotation = true
lombok.extern.findbugs.addSuppressFBWarnings = true

